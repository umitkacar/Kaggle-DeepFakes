# Environment variables for DeepFake Detector
# Copy this file to .env and update with your settings

# Paths
DFD_LOG_DIR=./logs/dtn
DFD_DATA_DIR=./data/train
DFD_DATA_DIR_VAL=./data/val

# Mode
DFD_MODE=training

# GPU
DFD_GPU_USAGE=1
DFD_GPU_MEMORY_GROWTH=true

# Logging
DFD_LOG_LEVEL=INFO
DFD_VERBOSE=false

# Training
DFD_TRAINING__BATCH_SIZE=20
DFD_TRAINING__LEARNING_RATE=0.0001
DFD_TRAINING__MAX_EPOCHS=70
